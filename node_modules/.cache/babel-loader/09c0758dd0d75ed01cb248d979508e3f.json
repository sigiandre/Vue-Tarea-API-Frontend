,CAAC,GAAR,CAAY,QAAZ;;AACA,QAAA,MAAI,CAAC,aAAL;;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,KAAf;AACD,OANH,EAOG,KAPH,CAOS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,KAAf;AACD,OAVH;AAWD,KAnCM;AAoCP,IAAA,WApCO,uBAoCK,KApCL,EAoCY,EApCZ,EAoCgB;AAAA;;AACtB;AACA,WAAK,OAAL,GAAe,IAAf;AACA,MAAA,KAAK,CAAC,GAAN,CAAU,sCAAqC,EAA/C,EAAmD,KAAnD,EAA0D,IAA1D,CAA+D,YAAK;AAClE,QAAA,MAAI,CAAC,aAAL;;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,KAAf;AACD,OAHD,EAGG,KAHH,CAGS;AAAA,eAAM,MAAI,CAAC,OAAL,GAAe,KAArB;AAAA,OAHT;AAIA,KA3CM;AA4CP,IAAA,aA5CO,2BA4CS;AAAA;;AACd,WAAK,OAAL,GAAe,IAAf;AACA,MAAA,KAAK,CAAC,GAAN,CAAU,kCAAV,EAA8C,IAA9C,CAAmD,UAAC,QAAD,EAAc;AAC/D,QAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACA,QAAA,MAAI,CAAC,UAAL,GAAkB,QAAQ,CAAC,IAA3B;AACA,QAAA,MAAI,CAAC,OAAL,GAAe,KAAf;AACD,OAJD,EAIG,KAJH,CAIS;AAAA,eAAM,MAAI,CAAC,OAAL,GAAe,KAArB;AAAA,OAJT;AAKD;AAnDM,GATI;AA8Db,EAAA,OAAO,EAAE,mBAAY;AACnB,SAAK,aAAL;AACD;AAhEY,CAAf","sourcesContent":["<template>\n  <div>\n    <h1 class=\"display-4 text-center\">Listado de Tareas</h1>\n    <hr />\n    <div class=\"row\">\n      <div class=\"col-lg-8 offset-lg-2\">\n        <div class=\"card mt-4\">\n          <div class=\"card-body\">\n            <div class=\"input-group\">\n              <input type=\"text\" v-model=\"tarea\" class=\"form-control form-control-lg\" placeholder=\"Agregar Tarea \" />\n              <div class=\"input-group-append\">\n                <button v-on:click=\"agregarTarea()\" class=\"btn btn-success btn-lg\">Agregar</button>\n              </div>\n            </div>\n            <br/>\n            <div class=\"text-center\">\n              <div v-if=\"loading\" class=\"spinner-border text-success\" role=\"status\">\n                  <span class=\"sr-only\">Loading...</span>\n              </div>\n            </div>\n\n            <h5 v-if=\"listTareas.length == 0\">No hay tareas para realizar</h5>\n            <ul v-if=\"!loading\" class=\"list-group\">\n              <li v-for=\"(tarea, index) of listTareas\" :key=\"index\" class=\"list-group-item d-flex justify-content-between\">\n                <span class=\"cursor\" \n                    v-bind:class=\"{ 'text-success': tarea.estado }\" \n                    v-on:click=\"editarTarea(tarea, tarea.id)\">\n                  <i v-bind:class=\"[ tarea.estado ? 'fas fa-check-circle' : 'far fa-circle',]\"></i>\n                </span>\n                {{ tarea.nombre }}\n                <span class=\"text-danger cursor\" v-on:click=\"eliminarTarea(tarea.id)\">\n                    <i class=\"fas fa-trash-alt\"></i>\n                </span>\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"Tarea\",\n  data() {\n    return {\n      tarea: \"\",\n      listTareas: [],\n      loading: false,\n    };\n  },\n  methods: {\n    agregarTarea() {\n      const tarea = {\n        nombre: this.tarea,\n        estado: false,\n      };\n      /*  this.listTareas.push(tarea); */\n      this.loading = true;\n      axios\n        .post(\"https://localhost:5001/api/Tarea/\", tarea)\n        .then((response) => {\n          console.log(response);\n          this.loading = false;\n          this.obtenerTareas();\n        })\n        .catch((error) => {\n          console.error(error);\n          this.loading = false;\n        });\n      this.tarea = \"\";\n    },\n    eliminarTarea(id) {\n      /* this.listTareas.splice(index, 1) */\n      this.loading = true;\n      axios\n        .delete(\"https://localhost:5001/api/Tarea/\" + id)\n        .then((response) => {\n          console.log(response);\n          this.obtenerTareas();\n          this.loading = false;\n        })\n        .catch((error) => {\n          console.log(error);\n          this.loading = false;\n        });\n    },\n    editarTarea(tarea, id) {\n     /*  this.listTareas[index].estado = !tarea.estado; */\n     this.loading = true;\n     axios.put(\"https://localhost:5001/api/Tarea/\"+ id, tarea).then(()=> {\n       this.obtenerTareas();\n       this.loading = false\n     }).catch(() => this.loading = false);\n    },\n    obtenerTareas() {\n      this.loading = true;\n      axios.get(\"https://localhost:5001/api/Tarea\").then((response) => {\n        console.log(response);\n        this.listTareas = response.data;\n        this.loading = false;\n      }).catch(() => this.loading = false);\n    },\n  },\n  created: function () {\n    this.obtenerTareas();\n  },\n};\n</script>\n\n<style scoped>\n.cursor {\n  cursor: pointer;\n}\n</style>"],"sourceRoot":""}]}